import matplotlib.pyplot as plt
import numpy as np
x=np.arange(0,20,0.1)
y=np.sin(x)
z=np.tan(x)
fig=plt.figure(figsize=(10,10))
plt.plot(x,y)
plt.plot(x,z,marker="*")
plt.show()
